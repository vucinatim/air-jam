    "zod": "^3.24.1",
    "zustand": "^5.0.3"
  },
  "peerDependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@types/howler": "^2.2.12",
