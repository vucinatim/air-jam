{
  "name": "@air-jam/sdk",
  "version": "0.1.1",
  "description": "The core SDK for building Air Jam games and controllers",
  "private": false,
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vucinatim/air-jam.git",
    "directory": "packages/sdk"
  },
  "homepage": "https://github.com/vucinatim/air-jam#readme",
  "keywords": [
    "air-jam",
    "game",
    "multiplayer",
    "sdk",
    "react",
    "typescript",
    "controller",
    "mobile-controller",
    "real-time",
    "socket.io",
    "gaming"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./protocol": {
      "types": "./dist/protocol.d.ts",
      "import": "./dist/protocol.js",
      "require": "./dist/protocol.cjs"
    },
    "./styles.css": "./src/styles.css"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "dev": "tsup --watch"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "howler": "^2.2.4",
    "html5-qrcode": "^2.3.8",
    "lucide-react": "^0.554.0",
    "nanoid": "^5.1.6",
    "qrcode": "^1.5.4",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^2.5.5",
    "zod": "^4.2.1",
    "zustand": "^5.0.3"
  },
  "peerDependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@types/howler": "^2.2.12",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "tailwindcss": "^4.1.17",
    "tsup": "^8.5.1",
    "typescript": "~5.9.3"
  }
}
