{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",
    "rootDir": "src",
    "outDir": "dist",
    "composite": true,
    "incremental": true,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "noEmit": false,
    "tsBuildInfoFile": "dist/.tsbuildinfo",
    "types": ["react", "react-dom"]
  },
  "include": ["src"]
}
